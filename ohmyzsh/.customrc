DISABLE_AUTO_UPDATE="true"
# Example aliases
alias vi='vim'
alias cpmsgid='gitdir=$(git rev-parse --git-dir); scp -p -P 29418 admin@websrv:hooks/commit-msg ${gitdir}/hooks/'

# Set to this to use case-sensitive completion
